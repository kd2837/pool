
#include <stdio.h>

void my_putchar(char c)
{
   write(1, &c, 1);
 }

int main(void)
{

 
  int n;
  my_putchar(56);
  getchar();
    
   
  if (n < 0)
    {

     
      my_putchar(78);
     
    }

  if ( n >= 0)
    {
      my_putchar(80);
    }
  
  return (0);
}

